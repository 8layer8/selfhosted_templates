        "name": "PUID",
        "label": "PUID",
        "default": "1000"
      },
      {
        "name": "PGID",
        "label": "PGID",
        "default": "100"
      },
      {
        "name": "CONTEXT_PATH",
        "label": "CONTEXT_PATH",
        "set": "airsonic"
      },
      {
        "name": "JAVA_OPTS",
        "label": "JAVA_OPTS",
        "set": "-Xms256m -Xmx512m"
      }
    ]
